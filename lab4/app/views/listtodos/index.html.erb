<p id="notice"><%= notice %></p>

<h1>Listing Listtodos</h1>

<table>
  <thead>
    <tr>
      <th>Todo</th>
      <th>Due</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listtodos.each do |listtodo| %>
      <tr>
        <td><%= listtodo.todo %></td>
        <td><%= listtodo.due %></td>
        <td><%= link_to 'Show', listtodo %></td>
        <td><%= link_to 'Edit', edit_listtodo_path(listtodo) %></td>
        <td><%= link_to 'Destroy', listtodo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listtodo', new_listtodo_path %>
